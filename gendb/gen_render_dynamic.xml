				<value value="0x0" name="UNORM"/>
				<value value="0x1" name="SNORM"/>
				<value value="0x2" name="RTFORMAT"/>
			</bitfield>
			<bitfield pos="1" name="PRE_BLEND_CLAMP"/>
			<bitfield pos="0" name="POST_BLEND_CLAMP"/>
		</reg32>
	</stripe>
</domain>

