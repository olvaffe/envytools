	<bitfield high="24" low="20" name="MSG_RLEN"/>
	<bitfield pos="19" name="MSG_HEADER_PRESENT"/>
	<bitfield high="18" low="0" name="MSG_FUNCTION_CONTROL"/>
</bitset>

